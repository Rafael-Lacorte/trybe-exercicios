db.bios.findOne({_id: 8})
db.bios.findOne({_id: 8}, {_id: 1, name: 1 })
db.bios.findOne({_id: 8}, {_id: 0, name: 1, birth: 1 })
db.bios.find({"name.first": "John"}).pretty()
db.bios.find().limit(3).pretty()
db.bios.find().limit(2).skip(5)
db.book.countDocuments({})
db.book.find({ status: "PUBLISH"}).pretty()
db.book.find({}, {_id: 0, title: 1, isbn: 1, pageCount: 1 }).limit(3)
db.book.find({status: "MEAP"}, {_id: 0, title: 1, authors: 1, status: 1}).skip(5).limit(10).pretty()